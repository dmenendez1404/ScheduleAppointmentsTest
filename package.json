  },
  "homepage": "https://github.com/dmenendez1404/ScheduleAppointmentsTest#readme",
  "devDependencies": {
    "cz-conventional-changelog": "^3.0.2",
    "semantic-release": "^16.0.2"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
