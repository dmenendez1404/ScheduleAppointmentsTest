{
  "name": "app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@material-ui/core": "^4.8.3",
    "@material-ui/icons": "^4.5.1",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.4.0",
    "@testing-library/user-event": "^7.2.1",
    "@types/node": "^12.12.24",
    "@types/react": "^16.9.17",
    "@types/react-dom": "^16.9.4",
    "env-cmd": "^10.0.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-scripts": "3.3.0",
    "service-worker-mock": "^2.0.4",
    "typescript": "^3.7.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "start_qa": "env-cmd -f .env.qa react-scripts start",
    "start_prod": "env-cmd -f .env.production react-scripts start",
    "build": "react-scripts build",
    "build_qa": "env-cmd -f .env.qa react-scripts build",
    "build_prod": "env-cmd -f .env.production react-scripts build",
    "test": "react-scripts test ./src/.*.spec.* -u",
    "test:coverage": "react-scripts test ./src/.*.spec.* -u --coverage --watchAll",
    "test:coverage:ci": "react-scripts test ./src/.*.spec.* -u --ci --coverage --silent --watchAll=false",
    "eject": "react-scripts eject",
    "cypress:run": " cypress run",
    "cypress:open": "cypress open",
    "storybook": "start-storybook"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "jest": {
    "snapshotSerializers": [
      "enzyme-to-json"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@storybook/preset-create-react-app": "^1.5.1",
    "@storybook/react": "^5.3.2",
    "@types/enzyme": "^3.10.4",
    "@types/enzyme-adapter-react-16": "^1.0.5",
    "@types/jest": "^24.0.25",
    "cypress": "^3.8.2",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "enzyme-to-json": "^3.4.3",
    "jest": "^24.9.0",
    "raf": "^3.4.1",
    "ts-jest": "^24.3.0"
  }
}
