{
  "name": "app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@material-ui/core": "^4.8.3",
    "@material-ui/icons": "^4.5.1",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.4.0",
    "@testing-library/user-event": "^7.2.1",
    "@types/node": "^12.12.24",
    "@types/react": "^16.9.17",
    "@types/react-dom": "^16.9.4",
    "@types/react-redux": "^7.1.6",
    "axios": "^0.19.1",
    "classnames": "^2.2.6",
    "connected-react-router": "^6.6.1",
    "env-cmd": "^10.0.1",
    "history": "^4.10.1",
    "immutability-helper": "^3.0.1",
    "mui-datatables": "^2.14.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-loading-skeleton": "^1.3.0",
    "react-redux": "^7.1.3",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.3.0",
    "redux": "^4.0.5",
    "redux-action": "^1.2.2",
    "redux-logger": "^3.0.6",
    "redux-saga": "^1.1.3",
    "reduxsauce": "^1.1.1",
    "service-worker-mock": "^2.0.4",
    "typescript": "^3.7.4",
    "validator": "^12.1.0",
    "react-big-calendar": "0.20.4",
    "react-dnd": "7.4.5",
    "react-dnd-html5-backend": "7.4.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "start_qa": "env-cmd -f .env.qa react-scripts start",
    "start_prod": "env-cmd -f .env.production react-scripts start",
    "build": "react-scripts build",
    "build_qa": "env-cmd -f .env.qa react-scripts build",
    "build_prod": "env-cmd -f .env.production react-scripts build",
    "test": "react-scripts test ./src/.*.spec.* -u",
    "test:coverage": "react-scripts test ./src/.*.spec.* -u --coverage --watchAll",
    "test:coverage:ci": "react-scripts test ./src/.*.spec.* -u --ci --coverage --silent --watchAll=false",
    "eject": "react-scripts eject",
    "cypress:run": " cypress run",
    "cypress:open": "cypress open",
    "storybook": "start-storybook"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "jest": {
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}",
      "!**/index.{ts,tsx}",
      "!**/theme.ts",
      "!**/*.model.ts",
      "!**/*.stories.tsx",
      "!src/assets/**",
      "!src/react-app-env.d.ts"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@storybook/preset-create-react-app": "^1.5.1",
    "@storybook/react": "^5.3.2",
    "@types/classnames": "^2.2.9",
    "@types/enzyme": "^3.10.4",
    "@types/enzyme-adapter-react-16": "^1.0.5",
    "@types/jest": "^24.0.25",
    "@types/mui-datatables": "^2.13.4",
    "@types/react-router-dom": "^5.1.3",
    "@types/redux-logger": "^3.0.7",
    "@types/validator": "^12.0.1",
    "cypress": "^3.8.2",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "enzyme-to-json": "^3.4.3",
    "jest": "^24.9.0",
    "raf": "^3.4.1",
    "redux-mock-store": "^1.5.4",
    "ts-jest": "^24.3.0",
    "moment": "^2.24.0",
    "@storybook/addon-actions": "latest"
  }
}
